{
  "id": null,
  "title": "Call Center Operations",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "panels": [
    {
      "type": "stat",
      "title": "Queue Endpoint Latency",
      "id": 10,
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "avg_over_time(nokta_route_duration_seconds{route=\"GET /api/v1/call-center/queue\",stat=\"avg\"}[5m])",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 1 },
              { "color": "red", "value": 2 }
            ]
          }
        }
      },
      "gridPos": { "h": 6, "w": 12, "x": 0, "y": 0 }
    },
    {
      "type": "stat",
      "title": "Dashboard Error Rate",
      "id": 11,
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(increase(nokta_http_status_total{code=~\"5..\"}[15m]))",
          "refId": "A"
        }
      ],
      "gridPos": { "h": 6, "w": 12, "x": 12, "y": 0 }
    },
    {
      "type": "graph",
      "title": "Request Volume",
      "id": 12,
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "increase(nokta_http_requests_total[5m])",
          "legendFormat": "Requests",
          "refId": "A"
        }
      ],
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 6 }
    },
    {
      "type": "table",
      "title": "Route Performance",
      "id": 13,
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "nokta_route_duration_seconds",
          "format": "table",
          "refId": "A"
        }
      ],
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 14 }
    }
  ]
}
